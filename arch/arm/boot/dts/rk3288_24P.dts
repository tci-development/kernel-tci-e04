	status = "okay";
};
*/

&uart1 {
	status = "okay";
};
&uart3 {
	status = "okay";
};

&uart2 {
	status = "okay";
};

&hdmi_in_vopl {
	status = "disabled";
};

&lvds_in_vopb {
	status = "disabled";
};

&lvds {
	status = "okay";
};

&lvds_panel {
	status = "okay";
	compatible ="simple-panel";
	backlight = <&backlight>;
	bus-format = <MEDIA_BUS_FMT_RGB888_1X24>;
	enable-gpios = <&gpio7 4 GPIO_ACTIVE_HIGH>;
	enable-delay-ms = <10>;
	//pinctrl-0 = <&lcd_cs>;
	power-supply = <&vcc_lcd>;
	rockchip,data-mapping = "vesa";
	rockchip,data-width = <24>;
	rockchip,output = "duallvds";

	display-timings {
		native-mode = <&timing0>;

		timing0: timing0 {
			clock-frequency = <142400000>;
			hactive = <1920>;
			vactive = <1080>;
			hfront-porch = <1>;
			hsync-len = <1>;
			hback-porch = <90>;
			vfront-porch = <1>;
			vsync-len = <1>;
			vback-porch = <50>;
			hsync-active = <0>;
			vsync-active = <0>;
			de-active = <0>;
			pixelclk-active = <0>;
		};
	};
};



&pinctrl {
	act8846 {
		pmic_vsel: pmic-vsel {
			rockchip,pins = <7 14 RK_FUNC_GPIO &pcfg_output_low>;
		};

		pwr_hold: pwr-hold {
			rockchip,pins = <0 6 RK_FUNC_GPIO &pcfg_output_high>;
		};
	};
	backlight {
		bl_en: bl-en {
			rockchip,pins = <7 2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	headphone {
		hp_det: hp-det {
			rockchip,pins = <5 19 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

	lcd {
		lcd_en: lcd-en  {
			rockchip,pins = <7 3 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		lcd_cs: lcd-cs {
			rockchip,pins = <7 4 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	   };
};
